generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model User {
  id        String    @id @default(cuid())
  email     String    @unique
  password  String
  name      String?
  nickname  String?
  phone     String?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @default(now())
  deletedAt DateTime?
}

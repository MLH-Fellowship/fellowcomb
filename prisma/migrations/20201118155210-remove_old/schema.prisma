generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model pod {
  title String
  id    Int    @id @default(autoincrement()) @map("_id")
  user  user[]
}

model user {
  name  String
  podId Int?
  id    Int    @id @default(autoincrement()) @map("_id")
  pod   pod?   @relation(fields: [podId], references: [id])
}

{
  "version": "1.0.1",
  "description": "Graphback runtime template with Apollo Server and PostgreSQL",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn generate:prisma && yarn generate:nexus && tsc",
    "develop": "ts-node-dev src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "generate:prisma": "prisma generate",
    "migrate:save": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "migrate:down": "prisma migrate down 1 --experimental"
  },
  "license": "Apache 2.0",
  "dependencies": {
    "@nexus/schema": "^0.18.0",
    "@octokit/auth-app": "^2.10.2",
    "@octokit/auth-oauth-app": "^3.0.6",
    "@octokit/rest": "^18.0.9",
    "@prisma/cli": "^2.11.0",
    "@prisma/client": "^2.11.0",
    "apollo-server-express": "2.19.0",
    "axios": "^0.21.0",
    "cors": "2.8.5",
    "discord.js": "^12.4.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.4.1",
    "graphql-tag": "2.11.0",
    "nexus-plugin-prisma": "^0.24.0"
  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/graphql": "14.2.3",
    "@types/node-fetch": "2.5.7",
    "graphql-cli": "4.1.0",
    "ts-node": "9.0.0",
    "ts-node-dev": "^1.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.3"
  },
  "author": {
    "name": "Utkarsh Goel"
  }
}
